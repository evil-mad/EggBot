                                           ; *******************************************************************            source\squareroot.s
                                           ; The function of this square root routine is to determine the root              source\squareroot.s
                                           ; to the nearest integer. At the same time the root is found at the              source\squareroot.s
                                           ; best possible speed; therefore, the root is found a little differently         source\squareroot.s
                                           ; for the two basic sizes of numbers, 16-bit and 32-bit. The following           source\squareroot.s
                                           ; differentiates the two and jumps to the appropriate function.                  source\squareroot.s
                                           ; Sqrt(ARGA3:ARGA2:ARGA1:ARGA0) = RES1:RES0                                      source\squareroot.s
                                           	CODE                                                                            source\squareroot.s
                                           Sqrt	                                                                            source\squareroot.s
0090c2   0105     MOVLB     0x5            	movlb	5		; All our RAM is in bank 5                                             source\squareroot.s
0090c4   66ee     TSTFSZ    0xee,0x0       	tstfsz	ARGA3, A	; determine if the number is 16-bit                             source\squareroot.s
0090c6   d024     BRA       0x9110         	bra	Sqrt32		; or 32-bit and call the best function                              source\squareroot.s
0090c8   66ed     TSTFSZ    0xed,0x0       	tstfsz	ARGA2, A                                                                 source\squareroot.s
0090ca   d022     BRA       0x9110         	bra	Sqrt32                                                                      source\squareroot.s
0090cc   6af5     CLRF      0xf5,0x0       	clrf	RES1, A                                                                    source\squareroot.s
0090ce   d000     BRA       0x90d0         	bra	Sqrt16                                                                      source\squareroot.s
                                           	                                                                                source\squareroot.s
                                           	GLOBAL	Sqrt                                                                     source\squareroot.s
                                                                                                                            source\squareroot.s
                                           ; *******************************************************************            source\squareroot.s
                                           ; ******************** Square Root **********************************            source\squareroot.s
                                           ; Sqrt16(ARGA1:ARGA0) = RES0                                                     source\squareroot.s
                                           Sqrt16	                                                                          source\squareroot.s
0090d0   0105     MOVLB     0x5            	movlb	5		; All our RAM is in bank 5                                             source\squareroot.s
0090d2   6afd     CLRF      0xfd,0x0       	clrf	TEMP0, A	; clear the temp solution                                         source\squareroot.s
0090d4   0e80     MOVLW     0x80           	movlw	0x80		; setup the first bit                                               source\squareroot.s
0090d6   6efb     MOVWF     0xfb,0x0       	movwf	BITLOC0, A                                                                source\squareroot.s
0090d8   6ef6     MOVWF     0xf6,0x0       	movwf	RES0, A                                                                   source\squareroot.s
0090da   50f6     MOVF      0xf6,0x0,0x0   Square8 movf	RES0, W, A	; square the guess                                       source\squareroot.s
0090dc   02f6     MULWF     0xf6,0x0       	mulwf	RES0, A                                                                   source\squareroot.s
0090de   50f3     MOVF      0xf3,0x0,0x0   	movf	PRODL, W, A	; ARGA - PROD test                                             source\squareroot.s
0090e0   5ceb     SUBWF     0xeb,0x0,0x0   	subwf	ARGA0, W, A                                                               source\squareroot.s
0090e2   50f4     MOVF      0xf4,0x0,0x0   	movf	PRODH, W, A                                                                source\squareroot.s
0090e4   58ec     SUBWFB    0xec,0x0,0x0   	subwfb	ARGA1, W, A                                                              source\squareroot.s
0090e6   b0d8     BTFSC     0xd8,0x0,0x0   	btfsc	STATUS, C, A                                                              source\squareroot.s
0090e8   d008     BRA       0x90fa         	bra	NextBit		; if positive then next bit                                        source\squareroot.s
                                           				; if negative then rotate right                                              source\squareroot.s
0090ea   c1fd     MOVFF     0x1fd,0x1f6    	movff	TEMP0, RES0	; move last good value back into RES0                         source\squareroot.s
0090ec   f1f6                                                                                                               
0090ee   42fb     RRNCF     0xfb,0x1,0x0   	rrncf	BITLOC0, F, A	; then rotote the bit and put it                            source\squareroot.s
0090f0   50fb     MOVF      0xfb,0x0,0x0   	movf	BITLOC0, W, A	; back into RES0                                             source\squareroot.s
0090f2   12f6     IORWF     0xf6,0x1,0x0   	iorwf	RES0, F, A                                                                source\squareroot.s
0090f4   befb     BTFSC     0xfb,0x7,0x0   	btfsc	BITLOC0, 7, A	; if last value was tested then get                         source\squareroot.s
0090f6   d009     BRA       0x910a         	bra	Done		; out                                                                 source\squareroot.s
0090f8   d7f0     BRA       0x90da         	bra	Square8		; elso go back for another test                                    source\squareroot.s
0090fa   c1f6     MOVFF     0x1f6,0x1fd    NextBit movff	RES0, TEMP0	; copy the last good approximation                     source\squareroot.s
0090fc   f1fd                                                                                                               
0090fe   42fb     RRNCF     0xfb,0x1,0x0   	rrncf	BITLOC0, F, A	; rotate the bit location register                          source\squareroot.s
009100   50fb     MOVF      0xfb,0x0,0x0   	movf	BITLOC0, W, A                                                              source\squareroot.s
009102   12f6     IORWF     0xf6,0x1,0x0   	iorwf	RES0, F, A                                                                source\squareroot.s
009104   befb     BTFSC     0xfb,0x7,0x0   	btfsc	BITLOC0, 7, A	; if last value was tested then get                         source\squareroot.s
009106   d001     BRA       0x910a         	bra	Done		; out                                                                 source\squareroot.s
009108   d7e8     BRA       0x90da         	bra	Square8                                                                     source\squareroot.s
00910a   c1fd     MOVFF     0x1fd,0x1f6    Done	movff	TEMP0,RES0	; put the final result in RES0                             source\squareroot.s
00910c   f1f6                                                                                                               
00910e   0012     RETURN    0x0            	return                                                                          source\squareroot.s
